// function timesFive that accepts one argument
function timesFive(a) 
{
    a = a * 5;  //accepts one argument, multiplies it by 5
    return a;   //returns the new value
}

console.log(timesFive(5));  //timesFive(5) will return 25

console.log(timesFive(2));  //timesFive(2) will return 10

console.log(timesFive(0));  //timesFive(0) will return 0
